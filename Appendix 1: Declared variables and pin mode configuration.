#include <LiquidCrystal.h>
const int in4 = 0, in3 = 1, in2 = 2, in1 = 3, ena = A1, enb = A2;
int seconds = 0;
int normalSpeed = 255;                                                
LiquidCrystal lcd(8, 9, 4, 5, 6, 7);


void setup() {
  // set pin modes
  pinMode(in4, OUTPUT);
  pinMode(in3, OUTPUT);
  pinMode(in2, OUTPUT);
  pinMode(in1, OUTPUT);
  pinMode(ena, OUTPUT);
  pinMode(enb, OUTPUT);


  // set up the LCD's number of columns and rows:
  lcd.begin(16, 1);
}
